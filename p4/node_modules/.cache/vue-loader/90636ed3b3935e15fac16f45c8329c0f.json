n @click='removeFromLike(item.id)' class=\"uk-button uk-button-default uk-button-small uk-width-1-1\">Remove</button>\n                    <transition name='fade'>\n                      <div class='uk-alert-danger' v-if='addAlert'>Your like has been removed!</div>\n                  </transition>\n\n                </div>\n            </div>\n            </div>\n            </router-link>\n            </div>\n        </div>\n\n    </div>\n</template>\n\n<script>\nimport * as app from './../../app.js';\n\nexport default {\n    name: 'LikePage',\n    data: function() {\n        return {\n            items: [],\n            like: null,\n            albums: []\n        };\n    },\n    methods: {\n        getAlbumDetails(albumId) {\n            return this.albums.find(({ id }) => id === albumId);\n        },\n        removeFromLike: function(albumId) {\n            this.like.remove(albumId);\n        }\n    },\n    mounted() {\n        this.like = new app.Like();\n\n        this.items = this.like.getItems();\n\n        this.albums = app.axios\n            .get(app.config.api)\n            .then(response => (this.albums = response.data));\n    }\n};\n</script>\n"]}]}